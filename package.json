{
  "name": "nano-vue-i18n",
  "version": "1.2.0",
  "description": "A stupidly simple yet ultra-lightweight i18n plugin for Vue 3, designed for high-frequency rendering scenarios.",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "jsdom": "^24.1.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^1.6.1",
    "vue": "^3.5.27",
    "vue-i18n": "^11.2.8"
  },
  "keywords": [
    "vue",
    "i18n",
    "performance",
    "lightweight"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Anson2251/nano-vue-i18n.git"
  },
  "author": "Anson2251",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Anson2251/nano-vue-i18n/issues"
  },
  "homepage": "https://github.com/Anson2251/nano-vue-i18n#readme"
}
